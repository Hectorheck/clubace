{
    "name": "admire2/laravel",
    "type": "starter-kit",
    "description": "Admire Laravel starter kit",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "Envato License",
    "require": {
        "php": "^7.3| 8.0.12",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "infyomlabs/adminlte-templates": "8.0.x-dev",
        "infyomlabs/generator-builder": "dev-master",
        "infyomlabs/laravel-generator": "8.0.x-dev",
        "laracasts/flash": "^3.2",
        "laravel/framework": "^8.0",
        "laravel/jetstream": "^1.6",
        "laravel/sanctum": "^2.8",
        "laravel/tinker": "^2.0",
        "livewire/livewire": "^2.0",
        "maatwebsite/excel": "^3.1",
        "phpoffice/phpspreadsheet": "^1.18",
        "transbank/transbank-sdk": "~2.0",
        "yajra/laravel-datatables-buttons": "^4.9",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
